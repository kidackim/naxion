        "cypress": "6.2.1",
        "cypress-file-upload": "^4.1.1",
        "cypress-mochawesome-reporter": "^1.3.0",
        "cypress-multi-reporters": "^1.4.0",
        "mocha-junit-reporter": "^2.0.0"
    }
}
